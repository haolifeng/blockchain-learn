<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>User List</title>
    <link rel='stylesheet' href='/bootstrap-3.3.7/css/bootstrap.css' />
</head>
<body>
<script src="/javascript/jquery-3.3.1.min.js"></script>
<script src="/bootstrap-3.3.7/js/bootstrap.js" type="text/javascript"></script>
<table class="table table-striped">
    <caption>Card Lists</caption>
    <thead>
    <tr>
        <th>ObjectId</th>
        <th>Card Id</th>
        <th>Card Address</th>
        <th>Card Image</th>
        <th>Card Owner</th>
        <th>Card Qrcode</th>
        <th>Card Details</th>

    </tr>
    </thead>
    <tbody>
    <% docs.forEach(function(doc){%>
    <tr>
        <td><%=doc._id%></td>
        <td><%=doc.card_id%></td>
        <td><%=doc.card_address%></td>
        <td><%=doc.card_img%></td>
        <td><%=doc.card_own%></td>
        <td><%=doc.card_qrcode%></td>
        <td><a href="/ntf/carddetails?_id=<%=doc.id%>" >Details</a></td>


    </tr>
    <%});%>
    </tbody>

</table>

</body>
</html>